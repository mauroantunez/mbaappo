<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.app.mbaappo.mbaappo.CrearEditarServicio">

    <TextView
        android:id="@+id/tVTitulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/titulo_del_servicio"
        tools:layout_editor_absoluteY="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.077"
        tools:ignore="MissingConstraints"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="16dp" />

    <EditText
        android:id="@+id/etTituloServicio"
        android:layout_width="329dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/electricista"
        tools:layout_editor_absoluteY="70dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="MissingConstraints"
        android:layout_below="@+id/tVTitulo"
        android:layout_alignLeft="@+id/tVTitulo"
        android:layout_alignStart="@+id/tVTitulo"
        android:layout_marginTop="13dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Descripcion del Servicio"
        tools:layout_editor_absoluteY="156dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.091"
        tools:ignore="HardcodedText,MissingConstraints"
        android:layout_below="@+id/etTituloServicio"
        android:layout_alignLeft="@+id/editText2"
        android:layout_alignStart="@+id/editText2"
        android:layout_marginTop="13dp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="331dp"
        android:layout_height="94dp"
        android:ems="10"
        android:inputType="textMultiLine"
        tools:layout_editor_absoluteY="189dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:ignore="MissingConstraints"
        android:layout_below="@+id/etTituloServicio"
        android:layout_alignRight="@+id/etTituloServicio"
        android:layout_alignEnd="@+id/etTituloServicio"
        android:layout_marginTop="41dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/precio"
        tools:layout_editor_absoluteY="315dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        tools:ignore="MissingConstraints"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/etTituloServicio"
        android:layout_alignStart="@+id/etTituloServicio" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tarifa"
        tools:layout_editor_absoluteX="320dp"
        tools:layout_editor_absoluteY="315dp"
        tools:ignore="MissingConstraints"
        android:layout_alignBaseline="@+id/textView5"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignLeft="@+id/spTarifa"
        android:layout_alignStart="@+id/spTarifa" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="114dp"
        android:layout_height="35dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="guaranies"
        tools:layout_editor_absoluteY="346dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.032"
        tools:ignore="HardcodedText,MissingConstraints"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/textView5"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5" />

    <Spinner
        android:id="@+id/spTarifa"
        android:layout_width="113dp"
        android:layout_height="34dp"
        app:layout_constraintHorizontal_bias="0.925"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="346dp"
        android:layout_alignTop="@+id/editText4"
        android:layout_alignRight="@+id/editText2"
        android:layout_alignEnd="@+id/editText2" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/categoria"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="401dp"
        tools:ignore="MissingConstraints"
        android:layout_below="@+id/editText4"
        android:layout_alignLeft="@+id/editText4"
        android:layout_alignStart="@+id/editText4"
        android:layout_marginTop="38dp" />

    <Spinner
        android:id="@+id/spCateg"
        android:layout_width="257dp"
        android:layout_height="28dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="437dp"
        tools:ignore="MissingConstraints"
        android:layout_below="@+id/textView"
        android:layout_alignLeft="@+id/editText2"
        android:layout_alignStart="@+id/editText2"
        android:layout_marginTop="16dp"
        android:layout_alignRight="@+id/spTarifa"
        android:layout_alignEnd="@+id/spTarifa" />

    <Button
        android:id="@+id/btnPublicarServicio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="22dp"
        android:text="Publicar"
        tools:ignore="HardcodedText"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
